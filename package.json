{
  "name": "simple-javascript",
  "version": "0.0.1",
  "description": "utility for creating simple apps and libraries, - similar to create-react-app, but for small npm-modules and apps",
  "icon": "icon.png",
  "main": "dist.js",
  "ldoc": "simple-javascript.js",
  "bin": {
    "simple-javascript": "simple-javascript.js"
  },
  "scripts": {
    "release": "./simple-javascript.js release",
    "dist": "npm run build && npm run test && increment-patch-version",
    "dev": "webpack-dev-server",
    "test": "true || electron-testrunner dist.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/solsort/simple-javascript.git"
  },
  "author": "RasmusErik Voel Jensen <npmjs@solsort.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/solsort/simple-javascript/issues"
  },
  "homepage": "https://simple-javascript.solsort.com/",
  "dependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.23.0",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.2"
  },
  "devDependencies": {
    "electron-testrunner": "^0.1.0",
    "increment-patch-version": "^0.0.1"
  }
}
